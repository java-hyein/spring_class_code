<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<!-- 객체 생성임 -->
	<!-- Calculation.myCal  = new  com.javalec.ex.Calculation-->
	<!-- 여기 id 는 MyCalculation 에서 설정해준 Calculation 'cal'; 과 객체와 이름이 같아야한다. -->
	
	<bean id = "cal" class ="com.javalec.ex.Calculation" />
	
	<!-- MyCalculation myCal = new MyCalculation-->
	<!-- MyCalculation myCal = ctx.getBean("myCal", MyCalculation.class); -->
	<bean id = "myCal" class ="com.javalec.ex.MyCalculation" >
		<!-- myCal.setCal() -->
		<property name = "cal">
		<!--  <ref bean ="cal"/> ,,<bean id = "cal" 같아야 한다. 객체 넣어주기 -->
			<ref bean ="cal"/>
		</property>
		<!-- myCal.setFirstNum(10),  myCal.secondNum(2) --> 
		<property name = "firstNum" value = "10"></property>
		<property name = "secondNum" value = "2"></property>
		
	</bean>
</beans>
